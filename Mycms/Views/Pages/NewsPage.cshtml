@model NewsPageViewModel

<div class="container">

	<div class="text-center">
		<h1>@Html.PropertyFor(x => x.Page.HeadLine)</h1>
	</div>

	<div class="text-center">
		<p class="lead my-3">@Html.PropertyFor(x => x.Page.HeadLineSlug)</p>
	</div>

	<h2>contentArea - Page</h2>
	<section class="my-4">

		<div class="row d-flex justify-content-center" epi-property="Page.NewListArea">
			<div class="card col-3 mx-3">
				<div epi-property-item />
			</div>
		</div>
	</section>


	<h2>PartialView - ViewModel</h2>
	<section class="my-4">
		<div class="row d-flex justify-content-center">
		@foreach(NewsItemPage page in Model.newlist!){
			<div class="card col-3 mx-3">
				<partial name="~/Views/PartialPages/NewsItemPage.cshtml" model="page" />
			</div>
		}
		</div>
	</section>

	<script>
		console.log(123)
	</script>
</div>